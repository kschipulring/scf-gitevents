{{outlet}}

<div class="row force-down">
	<div class="panel panel-default">
		<div class="panel-heading">
			<h3 class="panel-title">Events</h3>
		</div>
		<div class="panel-body" data-event-group="events">
			{{#each events as |event|}}
				<a href="{{event.url}}" target="_blank" class="list-group-item ember-view" data-event-type="{{event.eventType}}">
					<b>ID:</b> {{event.id}}<br/>
					<b>type:</b> {{event.eventType}}<br/>
					<b>TimeStamp:</b> {{event.created_at}}<br/>

					<b>Actor:</b>
					<table class="table table-striped table-bordered">
						<thead>
							<th>
							avatar_url
							</th>
							<th>display_login</th>
							<th>gravatar_id</th>
							<th>id</th>
							<th>login</th>
							<th>url</th>
						</thead>
						<tbody>
							<td>
							<img class="avatar-url" src="{{event.actor.avatar_url}}" />
							</td>
							<td>{{event.actor.display_login}}</td>
							<td>{{event.actor.gravatar_id}}</td>
							<td>{{event.actor.id}}</td>
							<td>{{event.actor.login}}</td>
							<td><a href="{{event.actor.url}}" target="_blank">{{event.actor.url}}</a></td>
						</tbody>
					</table>
				</a>
			{{/each}}
		</div>
	</div>
</div>